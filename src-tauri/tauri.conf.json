{
    "$schema": "../node_modules/@tauri-apps/cli/config.schema.json",
    "productName": "raahe hidayat",
    "version": "0.1.0",
    "identifier": "com.prayer.dev",
    "build": {
        "frontendDist": "../out",
        "devUrl": "https://madni-prayer.vercel.app/",
        "beforeDevCommand": "npm run dev",
        "beforeBuildCommand": "npm run build:tauri"
    },
    "app": {
        "windows": [
            {
                "title": "raahe hidayat",
                "width": 800,
                "height": 600,
                "resizable": true,
                "fullscreen": false
            }
        ],
        "security": {
            "csp": null
        }
    },
    "bundle": {
        "active": true,
        "targets": "all",
        "icon": ["logo.png"]
    }
}
